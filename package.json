{
  "name": "miniature-guacamole",
  "version": "1.0.0",
  "description": "an Apple Music web player built with Next.js and MusicKit",
  "main": "now.json",
  "repository": "https://github.com/mcansh/miniature-guacamole",
  "author": "Logan McAnsh <logan@mcan.sh> (https://mcan.sh)",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=10.x"
  },
  "scripts": {
    "build": "next build",
    "start": "next start",
    "type-check": "tsc",
    "lint": "eslint ./ --ext .js,.ts,.tsx",
    "lint:css": "stylelint './**/*.js' './**/*.jsx' './**/*.tsx' './**/*.ts'",
    "test": "jest",
    "validate": "yarn lint && yarn lint:css && yarn type-check"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "sh scripts/ts-precommit.bash",
      "yarn lint:css",
      "git add"
    ]
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.19",
    "@fortawesome/free-solid-svg-icons": "5.9.0",
    "@fortawesome/react-fontawesome": "0.1.4",
    "@reach/portal": "0.2.1",
    "@sentry/browser": "5.5.0",
    "@zeit/next-source-maps": "0.0.4-canary.1",
    "babel-plugin-inline-react-svg": "1.1.0",
    "babel-plugin-root-import": "6.2.0",
    "babel-plugin-styled-components": "1.10.6",
    "body-scroll-lock": "2.6.3",
    "constate": "1.2.0",
    "isomorphic-unfetch": "3.0.0",
    "jsonwebtoken": "8.5.1",
    "ms": "2.1.2",
    "next": "9.0.0",
    "next-offline": "4.0.2",
    "next-server": "9.0.0",
    "nookies": "2.0.6",
    "polished": "3.4.1",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-no-ssr": "1.1.0",
    "react-simple-img": "2.3.1",
    "react-spring": "8.0.27",
    "react-use-gesture": "5.1.2",
    "styled-components": "5.0.0-beta.3",
    "ts-optchain": "0.1.7",
    "type-fest": "0.6.0"
  },
  "devDependencies": {
    "@testing-library/react": "8.0.4",
    "@types/body-scroll-lock": "2.6.0",
    "@types/jest": "24.0.15",
    "@types/jsonwebtoken": "8.3.2",
    "@types/ms": "0.7.30",
    "@types/react": "16.8.23",
    "@types/react-dom": "16.8.4",
    "@types/react-no-ssr": "1.1.1",
    "@types/styled-components": "4.1.16",
    "@typescript-eslint/eslint-plugin": "1.11.0",
    "@typescript-eslint/parser": "1.11.0",
    "eslint": "6.0.1",
    "eslint-config-airbnb": "17.1.1",
    "eslint-config-kentcdodds": "14.3.3",
    "eslint-config-mcansh": "1.1.1",
    "eslint-config-prettier": "6.0.0",
    "eslint-import-resolver-babel-plugin-root-import": "1.1.1",
    "eslint-import-resolver-typescript": "1.1.1",
    "eslint-plugin-import": "2.18.0",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-you-dont-need-lodash-underscore": "6.5.0",
    "husky": "3.0.0",
    "jest": "24.8.0",
    "jest-dom": "3.5.0",
    "lint-staged": "9.1.0",
    "prettier": "1.18.2",
    "stylelint": "10.1.0",
    "stylelint-config-recommended": "2.2.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.8.0",
    "typescript": "3.5.2"
  }
}
