{
  "name": "miniature-guacamole",
  "description": "an Apple Music web player built with Remix and MusicKit",
  "repository": "mcansh/miniature-guacamole",
  "author": "Logan McAnsh <logan@mcan.sh> (https://mcan.sh)",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=14.x"
  },
  "scripts": {
    "dev": "run-p dev:*",
    "dev:css": "tailwindcss -o app/styles/app.css --watch",
    "dev:remix": "remix dev",
    "build": "run-s build:*",
    "build:css": "tailwindcss -o app/styles/app.css --minify",
    "build:remix": "remix build",
    "start": "remix-serve build",
    "type-check": "tsc",
    "lint": "eslint ./ --ext .js,.ts,.tsx",
    "test": "vitest",
    "validate": "yarn lint && yarn type-check"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "6.1.2",
    "@fortawesome/free-solid-svg-icons": "6.1.2",
    "@fortawesome/react-fontawesome": "0.2.0",
    "@mcansh/remix-fastify": "1.12.0",
    "@mcansh/remix-secure-headers": "0.6.2",
    "@remix-run/node": "0.0.0-experimental-9b7f37c9a",
    "@remix-run/react": "0.0.0-experimental-9b7f37c9a",
    "@remix-run/serve": "0.0.0-experimental-9b7f37c9a",
    "@sentry/browser": "7.16.0",
    "@yujinakayama/apple-music": "0.4.1",
    "cachified": "2.0.0",
    "clsx": "1.2.1",
    "date-fns": "2.29.3",
    "fastify": "4.9.2",
    "jsonwebtoken": "8.5.1",
    "lru-cache": "7.14.0",
    "ms": "2.1.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "type-fest": "3.1.0"
  },
  "devDependencies": {
    "@remix-run/dev": "0.0.0-experimental-9b7f37c9a",
    "@remix-run/eslint-config": "0.0.0-experimental-9b7f37c9a",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@types/jsonwebtoken": "8.5.9",
    "@types/ms": "0.7.31",
    "@types/musickit-js": "1.0.4",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "@vitejs/plugin-react": "2.1.0",
    "autoprefixer": "10.4.12",
    "dotenv": "16.0.3",
    "eslint": "8.26.0",
    "eslint-plugin-prefer-let": "3.0.1",
    "happy-dom": "7.6.0",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.18",
    "prettier": "2.7.1",
    "prettier-plugin-tailwindcss": "0.1.13",
    "tailwindcss": "3.2.1",
    "typescript": "4.8.4",
    "vite": "3.1.8",
    "vite-tsconfig-paths": "3.5.2",
    "vitest": "0.24.3"
  }
}
